generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Request {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String
  email     String
  service   String
  message   String?
  status    String   @default("new")
  createdAt DateTime @default(now())
}
